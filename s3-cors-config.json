{
  "CORSRules": [
    {
      "AllowedOrigins": [
        "http://localhost:3000",
        "http://localhost:3001",
        "https://your-production-domain.com"
      ],
      "AllowedMethods": [
        "GET",
        "HEAD"
      ],
