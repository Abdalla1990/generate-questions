{
  "name": "questions-api",
  "version": "1.0.0",
  "description": "API for questions generation and management",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "swc src -d dist --copy-files",
    "start": "node dist/server.js",
    "dev": "concurrently \"swc src -d dist --copy-files --watch\" \"nodemon --delay 500ms dist/server.js\"",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.95",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}